{
    "algo_name": "diffusion_policy",
    "experiment": {
        "name": "bc_pointcloud_diffusion_policy",
        "validate": false,
        "logging": {
            "terminal_output_to_txt": true,
            "log_tb": true,
            "log_wandb": true,
            "wandb_proj_name": "bc_pointcloud_diffusion"
        },
        "save": {
            "enabled": true,
            "every_n_epochs": 50
        },
        "epoch_every_n_steps": 100,
        "validation_epoch_every_n_steps": 10,
        "rollout": {
            "enabled": false,
            "n": 50,
            "horizon": 400,
            "rate": 50,
            "warmstart": 0,
            "terminate_on_success": true
        }
    },
    "train": {
        "data": "/home/raymond/projects/robomimic_dataset.hdf5",
        "output_dir": "/home/raymond/projects/bc_trained_models",
        "normalize_weights_by_ds_size": false,
        "num_data_workers": 4,
        "hdf5_cache_mode": "all",
        "hdf5_use_swmr": false,
        "hdf5_load_next_obs": false,
        "hdf5_normalize_obs": true,
        "hdf5_filter_key": null,
        "hdf5_validation_filter_key": null,
        "seq_length": 4,
        "pad_seq_length": true,
        "frame_stack": 1,
        "pad_frame_stack": true,
        "dataset_keys": [
            "actions",
            "rewards",
            "dones"
        ],
        "action_keys": [
            "actions"
        ],
        "action_config": {
            "actions": {
                "normalization": null
            }
        },
        "goal_mode": null,
        "cuda": true,
        "batch_size": 64,
        "num_epochs": 2000,
        "seed": 1,
        "max_grad_norm": null
    },
    "algo": {
        "optim_params": {
            "policy": {
                "optimizer_type": "adamw",
                "learning_rate": {
                    "initial": 0.0001,
                    "decay_factor": 0.1,
                    "step_every_batch": true,
                    "scheduler_type": "cosine",
                    "num_cycles": 0.5,
                    "warmup_steps": 500,
                    "epoch_schedule": []
                },
                "regularization": {
                    "L2": 1e-06
                }
            }
        },
        "horizon": {
            "observation_horizon": 2,
            "action_horizon": 1,
            "prediction_horizon": 4
        },
        "unet": {
            "enabled": true,
            "diffusion_step_embed_dim": 256,
            "down_dims": [
                256,
                512
            ],
            "kernel_size": 5,
            "n_groups": 8
        },
        "ema": {
            "enabled": true,
            "power": 0.75
        },
        "ddpm": {
            "enabled": true,
            "num_train_timesteps": 100,
            "num_inference_timesteps": 100,
            "beta_schedule": "squaredcos_cap_v2",
            "clip_sample": true,
            "prediction_type": "epsilon"
        },
        "ddim": {
            "enabled": false,
            "num_train_timesteps": 100,
            "num_inference_timesteps": 10,
            "beta_schedule": "squaredcos_cap_v2",
            "clip_sample": true,
            "set_alpha_to_one": true,
            "steps_offset": 0,
            "prediction_type": "epsilon"
        }
    },
    "observation": {
        "modalities": {
            "obs": {
                "low_dim": [
                    "joint_positions",
                    "joint_velocities",
                    "gripper_position",
                    "cartesian_position"
                ],
                "rgb": [],
                "depth": [],
                "scan": [
                    "pointcloud"
                ]
            },
            "goal": {
                "low_dim": [],
                "rgb": [],
                "depth": [],
                "scan": []
            }
        },
        "encoder": {
            "scan": {
                "core_class": "PointNetEncoder",
                "core_kwargs": {
                    "feature_dimension": 256,
                    "use_normals": false
                },
                "obs_randomizer_class": "PointCloudRandomizer",
                "obs_randomizer_kwargs": {
                    "rotation_range": 0.2,
                    "translation_range": 0.06,
                    "shuffle_points": true,
                    "num_samples": 1
                }
            },
            "low_dim": {
                "core_class": null,
                "core_kwargs": {},
                "obs_randomizer_class": null,
                "obs_randomizer_kwargs": {}
            },
            "rgb": {
                "core_class": "VisualCore",
                "core_kwargs": {
                    "feature_dimension": 64,
                    "flatten": true,
                    "backbone_class": "ResNet18Conv",
                    "backbone_kwargs": {
                        "pretrained": false,
                        "input_coord_conv": false
                    },
                    "pool_class": "SpatialSoftmax",
                    "pool_kwargs": {
                        "num_kp": 32,
                        "learnable_temperature": false,
                        "temperature": 1.0,
                        "noise_std": 0.0
                    }
                },
                "obs_randomizer_class": null,
                "obs_randomizer_kwargs": {}
            },
            "depth": {
                "core_class": "VisualCore",
                "core_kwargs": {},
                "obs_randomizer_class": null,
                "obs_randomizer_kwargs": {}
            }
        }
    },
    "meta": {
        "hp_base_config_file": null,
        "hp_keys": [],
        "hp_values": []
    }
}
